<div class="container">
    <h1>Shop Shoes Today!</h1>
        <div class="card-deck">
        <% @listings.each do |listing| %>
        <div class="card mb-4 shadow-sm">
        <div class="card-header">
        <%= image_tag(listing.images[0]) %>
        <%#= listing.images[0..2] %>
        <%# if @listings.images.attached? %>
        <%# @listings.images.each do |image| %>
        <%#= image_tag(image) %>
        <%# end %>
        <%# end %>
        </div>
        
          <div class="card-body">
            <h5 class="card-title"><%= listing.name %></h5>
            <h6 class="card-subtitle mb-2 text muted"> $ <%= listing.price %></h6>
            <button type="button" class="btn btn-lg btn-block btn-outline-primary">Buy Now</button>
          </div>
        </div>
        <% end %>
</div>
</div>